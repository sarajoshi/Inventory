	}
	public void consumersArrive() {
		for (int i=1; i <= 31; ++i) {
			for(int j=1; j<=10; ++j) {
				Queue[] customers = new Queue[50];
				Random r = new Random();
				int k = r.nextInt(100-0);
				for (int a=0; a>Queue[]; ++i) {
					if (k > 50) {
						lostCustomerDay=50-k;
					}
				}
			}
		}
	}
	public int getShipment() {

		return bun;

